public class Test
{
public static void main(String[] args) {
String s="hello worrld";
// helo world
String[] s1=s.split(" ");
for(int i=0;i<s1.length;i++)
{
	if(s1[i]!=null)
	{
		String s2="";
		for(int j=0;j<s1[i].length()-1;j++)
		{
			if(s1[i].charAt(j)!=s1[i].charAt(j+1))
			{
				s2=s2+s1[i].charAt(j);
			}
		}
		s2=s2+s1[i].charAt(s1[i].length()-1);
		System.out.print(s2+" ");
	}
}
}
}
