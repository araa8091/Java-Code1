import java.util.Arrays;
public class Abhi {
	public static void main(String[] args) {
		int a=12345;
		int temp=0;
		int b[]=new int[6];
		for(int i=0;i<b.length;i++)
		{
			temp=a%10;
			a=a/10;
			b[i]=temp;
		}
		for(int i=0;i<b.length;i++)
		{
			System.out.print(b[i]+" ");
		}
		System.out.println();
		for(int i=b.length-1;i>=0;i--)
		{
			System.out.print(b[i]+" ");
		}
	}
}
